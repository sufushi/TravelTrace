<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ObliqueView">
        <attr name="type">
            <enum name="solid_color" value="0" />
            <enum name="linear_gradient" value="1" />
            <enum name="radial_gradient" value="2" />
            <enum name="image" value="3" />
        </attr>
        <attr name="angle">
            <enum name="LEFT_TO_RIGHT" value="0" />
            <enum name="RIGHT_TO_LEFT" value="1" />
            <enum name="TOP_TO_BOTTOM" value="2" />
            <enum name="BOTTOM_TO_TOP" value="3" />
            <enum name="LEFT_TOP_TO_RIGHT_BOTTOM" value="4" />
            <enum name="RIGHT_TOP_TO_LEFT_BOTTOM" value="5" />
            <enum name="RIGHT_BOTTOM_TO_LEFT_TOP" value="6" />
            <enum name="LEFT_BOTTOM_TO_RIGHT_TOP" value="7" />
        </attr>
        <attr name="starting_slant_angle" format="float" />
        <attr name="ending_slant_angle" format="float" />
        <attr name="basecolor" format="color" />
        <attr name="startcolor" format="color" />
        <attr name="endcolor" format="color" />
        <attr name="radius" format="integer" />
        <attr name="shadow_height" format="float" />
        <attr name="gradient" format="boolean" />
        <attr name="src" format="reference" />
    </declare-styleable>

    <declare-styleable name="FoldingCell">
        <attr name="backSideColor" format="color" />
        <attr name="animationDuration" format="integer" />
        <attr name="additionalFlipsCount" format="integer" />
        <attr name="cameraHeight" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandIndicator" format="reference"/>
        <attr name="collapseIndicator" format="reference"/>
        <attr name="expandToggleType" format="enum">
            <enum name="ImageButton" value="0"/>
            <enum name="TextView" value="1"/>
        </attr>
        <attr name="expandableTextId" format="reference"/>
        <attr name="expandCollapseToggleId" format="reference"/>
        <attr name="expandToggleOnTextClick" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FlyEditText">
        <attr name="fly_text_color" format="color" />
        <attr name="fly_text_start_size" format="dimension" />
        <attr name="fly_text_scale" format="float" />
        <attr name="fly_duration" format="integer" />
        <attr name="fly_type" format="enum">
            <enum name="flyup" value="0" />
            <enum name="flydown" value="1" />
        </attr>

    </declare-styleable>

    <declare-styleable name="DrawableTextView">
        <attr name="drawableLeft" format="reference"/>
        <attr name="drawableBottom" format="reference"/>
        <attr name="drawableRight" format="reference"/>
        <attr name="drawableTop" format="reference"/>
        <attr name="drawableWidth" format="dimension"/>
        <attr name="drawableHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TimelineView" >
        <attr name="marker" format="color|reference"/>
        <attr name="markerSize" format="dimension"/>
        <attr name="markerInCenter" format="boolean"/>
        <attr name="startLineColor" format="color"/>
        <attr name="endLineColor" format="color"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="dash" value="1" />
        </attr>
        <attr name="lineStyleDashGap" format="dimension" />
        <attr name="lineStyleDashLength" format="dimension" />
        <attr name="lineOrientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="linePadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SwipeStack">
        <attr name="allowed_swipe_directions" format="integer">
            <enum name="only_left" value="1"/>
            <enum name="only_right" value="2"/>
            <enum name="both" value="0"/>
        </attr>
        <attr name="animation_duration" format="integer"/>
        <attr name="stack_size" format="integer"/>
        <attr name="stack_spacing" format="dimension"/>
        <attr name="stack_rotation" format="integer"/>
        <attr name="swipe_rotation" format="float"/>
        <attr name="swipe_opacity" format="float"/>
        <attr name="scale_factor" format="float"/>
        <attr name="disable_hw_acceleration" format="boolean"/>
    </declare-styleable>
</resources>
